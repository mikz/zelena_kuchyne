<table class="admin">
  <thead>
    <tr>
      <td><%=t(:attr_registration_no)%></td>
      <td><%=t(:driver)%></td>
      <td><%=t(:attr_date)%></td>
      <td><%=t(:attr_cost)%></td>
      <td><%=t(:attr_amount)%></td>
    </tr>
  </thead>
  <tbody>
    <% @sums.each do |sum|%>
    <tr>
      <td><%= format "#{sum.car.registration_no} #{sum.car.note}"%></td>
      <td><%= format sum.user.login%></td>
      <td><%= format_date Date.parse(sum[:from])%> a≈æ <%= format_date Date.parse(sum[:to])%></td>
      <td><%= format_currency sum.cost%></td>
      <td><%= format_amount sum.amount, :unit => t(:fuel_unit)%></td>
    </tr>
    <%end%>
  </tbody>
</table>
