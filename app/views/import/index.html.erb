<h1>Import dat</h1>
<%= form_tag({:action => 'import'}, {:multipart => true}) %>
  <p><label for="file"><%= t(:file).mb_chars.capitalize%></label><br />
    <%= file_field_tag 'file' %></p>
  <p>
    <label for="file_format"><%= t(:importer).mb_chars.capitalize%></label><br />
    <select name="file_format">
      <option value="users_from_mysql"><%= t(:users_from_mysql)%></option>
      <option value="users_from_csv"><%= t(:users_from_csv)%></option>
      <option value="country_codes_from_text"><%= t(:country_codes_from_text)%></option>
    </select>
  </p>
  <p><input type="submit" value="Odeslat" /></p>
</form>