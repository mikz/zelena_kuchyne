<h1>Import dat</h1>
<%= form_tag({:action => 'import'}, {:multipart => true}) %>
  <p><label for="file"><%= locales[:file].chars.capitalize%></label><br />
    <%= file_field_tag 'file' %></p>
  <p>
    <label for="file_format"><%= locales[:importer].chars.capitalize%></label><br />
    <select name="file_format">
      <option value="users_from_mysql"><%= locales[:users_from_mysql]%></option>
      <option value="users_from_csv"><%= locales[:users_from_csv]%></option>
      <option value="country_codes_from_text"><%= locales[:country_codes_from_text]%></option>
    </select>
  </p>
  <p><input type="submit" value="Odeslat" /></p>
</form>